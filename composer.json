{
	"name" : "hnm/search",
	"type" : "n2n-module",
	"description" : "Search module for n2n",
	"keywords" : [
		"n2n-tmpl-module",
		"n2n-module",
		"n2n"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "MIT",
	"authors" : [
        {
			"name" : "Thomas GÃ¼nther",
			"email" : "guenther@hnm.ch"
		}
	],	
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"config" : {
		"vendor-dir" : "src/vendor",
		"allow-plugins": {
			"n2n/n2n-composer-module-installer": true
		}
	},
	"require" : {
		"n2n/n2n-composer-module-installer" : "^7.1.6",
        "n2n/n2n": "^7.3",
		"n2n/rocket" : "^2.0.16 || ^3 || ^4",
        "ext-curl": "*"
    },
	"require-dev" : {
		"n2n/hangar": "^8.0",
		"phpunit/phpunit" : "^9.6.29",
        "n2n/n2n-test" : "7.4.x-dev"
    },
	"autoload" : {
		"psr-4" : {
			"search\\" : "src/app/search"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"search\\" : "src/app/search"
		}
	}
}
